[32m+[m
[32m+[m[32mnamespace TheForgottenKnight[m
[32m+[m[32m{[m
[32m+[m	[32mpublic enum ButtonStatus[m
[32m+[m	[32m{[m
[32m+[m		[32mNeutral,[m
[32m+[m		[32mHover,[m
[32m+[m		[32mClicked[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/// <summary>[m
[32m+[m	[32m/// Button compontent to display a button and execute a specified action[m
[32m+[m	[32m/// </summary>[m
[32m+[m	[32mpublic class ButtonComponent : DrawableGameComponent[m
[32m+[m	[32m{[m
[32m+[m		[32m/// <summary>[m
[32m+[m		[32m/// OnClick delegate to allow the function of the button to be defined through the constructor[m
[32m+[m		[32m/// </summary>[m
[32m+[m		[32mpublic delegate void OnClick();[m
[32m+[m
[32m+[m
[32m+[m		[32mprivate ButtonStatus buttonStatus;[m
[32m+[m		[32mprivate Vector2 position;[m
[32m+[m		[32mprivate Rectangle boundaries;[m
[32m+[m		[32mprivate Texture2D tex;[m
[32m+[m		[32mprivate OnClick onClick;[m
[32m+[m
[32m+[m		[32m/// <summary>[m
[32m+[m		[32m/// Create a button with a specified position texture and OnClick acction[m
[32m+[m		[32m/// </summary>[m
[32m+[m		[32m/// <param name="game">The game context for the ButtonComponent</param>[m
[32m+[m		[32m/// <param name="position">The postion of the button's top left corner</param>[m
[32m+[m		[32m/// <param name="tex">The image used for the button</param>[m
[32m+[m		[32m/// <param name="onClick">The OnClick fuction to be executed</param>[m
[32m+[m		[32mpublic ButtonComponent(Game game, Vector2 position, Texture2D tex, OnClick onClick) : base(game)[m
[32m+[m		[32m{[m
[32m+[m			[32mthis.tex = tex;[m
[32m+[m			[32mthis.onClick = onClick;[m
[32m+[m
[32m+[m			[32mthis.position = position + Shared.displayPosShift;[m
[32m+[m[41m			[m
[32m+[m			[32mboundaries = new Rectangle((int)this.position.X, (int)this.position.Y, tex.Width, tex.Height);[m
[32m+[m
[32m+[m			[32mbuttonStatus = ButtonStatus.Neutral;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic override void Update(GameTime gameTime)[m
[32m+[m		[32m{[m
[32m+[m			[32mMouseState ms = Mouse.GetState();[m
[32m+[m			[32mPoint mousePoint = new Point(ms.X, ms.Y);[m
[32m+[m
[32m+[m			[32m// Check if the button is hovered over by the mouse and clicked[m
[32m+[m			[32mif (boundaries.Contains(mousePoint))[m
[32m+[m			[32m{[m
[32m+[m				[32mbuttonStatus = ButtonStatus.Hover;[m
[32m+[m
[32m+[m				[32mif (ms.LeftButton == ButtonState.Pressed && buttonStatus== ButtonStatus.Hover)[m
[32m+[m				[32m{[m
[32m+[m					[32mbuttonStatus = ButtonStatus.Clicked;[m
[32m+[m					[32monClick();[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m
[32m+[m			[32mbase.Update(gameTime);[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic override void Draw(GameTime gameTime)[m
[32m+[m		[32m{[m
[32m+[m			[32mShared.sb.Begin();[m
[32m+[m
[32m+[m			[32mShared.sb.Draw(tex, boundaries, Color.White);[m
[32m+[m
[32m+[m			[32mShared.sb.End();[m
[32m+[m
[32m+[m
[32m+[m			[32mbase.Draw(gameTime);[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
